{
  "name": "cli-itu",
  "description": "A CLI for all things ITU related",
  "version": "0.0.0-development",
  "author": "Asger Rud Hansen @AsgereDreemurr",
  "bin": {
    "itu": "./bin/run"
  },
  "bugs": "https://github.com/AsgereDreemurr/CLI-ITU/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.7.4",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "semantic-release": "^17.4.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/AsgereDreemurr/CLI-ITU",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "itu",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "AsgereDreemurr/CLI-ITU",
  "scripts": {
    "commit": "git-cz",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release --branches main"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "cli-ux": "^5.6.3",
    "inquirer": "^8.1.2",
    "shelljs": "^0.8.4",
    "tslib": "^1.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
